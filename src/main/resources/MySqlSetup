create employee_db;

use employee_db;

CREATE TABLE employee (id int NOT NULL AUTO_INCREMENT, first_name VARCHAR(20), last_name VARCHAR(20),email VARCHAR(20), phone VARCHAR(20),PRIMARY KEY (id));

